#include <stdio.h>

main(){
	char c[2][3]={'a','s','d','f','g','h'};//定为2行3列的二维数组
	printf("%c\n",c[0][2]);//即为0行,2列的元素  返回'd'
	//a s d
	//f g h
	//内存中结构: c[0][0],c[0][1],c[0][2],c[1][0],c[1][1],c[1][2]
	//所谓的数组指针其实也称为行指针, 可以用来存放二维数组名
	//例如:
	char (*p)[3];
	p=c;
	//而:
	char *pp;
	//或者
	char *ppp[3];
	//pp=c;
	//ppp=c;
}
